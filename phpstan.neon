includes:
	- phpstan-baseline.neon

parameters:
	level: 9
	fileExtensions:
		- php
		- phtml
	paths:
		- src/
	treatPhpDocTypesAsCertain: false
	reportUnmatchedIgnoredErrors: false
	tmpDir: .tmp/phpstan
	typeAliases:
		CustomOrderFeeData: '''
			array{
					code: string,
					title: string,
					type: \JosephLeedy\CustomFees\Model\FeeType|value-of<\JosephLeedy\CustomFees\Model\FeeType>,
					percent: float|null,
					show_percentage: bool,
					base_value: float,
					value: float,
					base_value_with_tax: float,
					value_with_tax: float,
					base_tax_amount: float,
					tax_amount: float,
					tax_rate: float,
					base_discount_amount?: float,
					discount_amount?: float,
					discount_rate?: float,
			}
		'''
		CustomInvoiceFeeData: '''
			array{
					invoice_id?: int,
					code: string,
					title: string,
					type: \JosephLeedy\CustomFees\Model\FeeType|value-of<\JosephLeedy\CustomFees\Model\FeeType>,
					percent: float|null,
					show_percentage: bool,
					base_value: float,
					value: float,
					base_value_with_tax: float,
					value_with_tax: float,
					base_tax_amount: float,
					tax_amount: float,
					tax_rate: float,
					base_discount_amount?: float,
					discount_amount?: float,
					discount_rate?: float,
			}
		'''
		CustomCreditMemoFeeData: '''
			array{
					credit_memo_id?: int,
					code: string,
					title: string,
					type: \JosephLeedy\CustomFees\Model\FeeType|value-of<\JosephLeedy\CustomFees\Model\FeeType>,
					percent: float|null,
					show_percentage: bool,
					base_value: float,
					value: float,
					base_value_with_tax: float,
					value_with_tax: float,
					base_tax_amount: float,
					tax_amount: float,
					tax_rate: float,
					base_discount_amount?: float,
					discount_amount?: float,
					discount_rate?: float,
			}
		'''
